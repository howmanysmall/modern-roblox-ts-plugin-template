{
	"name": "roblox-ts-plugin-serve-test",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"tree": {
		"$className": "DataModel",
		"ServerStorage": {
			"$className": "ServerStorage",
			"Plugin": {
				"$className": "Folder",
				"Main": { "$path": "./default.project.json" }
			}
		},
		"TestService": {
			"$className": "TestService",
			"$properties": { "ExecuteWithStudioRun": true },
			"$path": "test"
		}
	}
}
