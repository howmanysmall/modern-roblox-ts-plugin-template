{
	"name": "roblox-ts-plugin-release",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json",
		"**/__tests__",
		"**/__stories__",
		"**/*.storybook.ts",
		"**/*.storybook.tsx"
	],
	"tree": {
		"$path": "dist",
		"include": {
			"$path": "include",
			"node_modules": {
				"$className": "Folder",
				"@rbxts": { "$path": "node_modules/@rbxts" },
				"@flamework": { "$path": "node_modules/@flamework" }
			}
		}
	}
}
